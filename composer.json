{
    "name": "snowcookie/generate-schema",
    "description": "Generate schema from database",
    "type": "libary",
    "license": "MIT",
    "keywords": [
        "laravel",
        "schema",
        "database"
    ],
    "authors": [
        {
            "name": "Bing Yue Chen",
            "email": "snow.shanalike@gmail.com"
        }
    ],
    "homepage": "https://github.com/bingyue-chen/GenerateSchema",
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.3",
        "illuminate/support": "8.*",
        "illuminate/database": "8.*",
        "illuminate/console": "8.*",
        "illuminate/filesystem": "8.*"
    },
    "require-dev": {
        "box/spout": "^3.1",
        "mockery/mockery": "^1.4",
        "orchestra/testbench": "^6.2",
        "phpunit/phpunit": "^9.3"
    },
    "suggest": {
        "box/spout": "Required to use csv renderer or xlsx renderer. (^3.1)"
    },
    "autoload": {
        "psr-4": {
            "Snowcookie\\GenerateSchema\\": "src/GenerateSchema/"
        },
        "classmap": [
            "tests/migrations"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Snowcookie\\GenerateSchema\\Test\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
